def function(array): #[1,4]
    new_array = []
    for i in array:
        new_array.append(0)
    return helper(new_array, array, 0)


def helper(new_array, array, index):
    if(index == len(array)):
        print(new_array)
        return 
    else:
        new_array[index] = 0
        helper(new_array, array, index+1)

        new_array[index] = array[index]
        helper(new_array, array, index+1)

array = [1,4]
function(array)
